  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "coveralls": "^3.0.9",
    "jest": "^24.9.0",
    "shx": "^0.3.2",
